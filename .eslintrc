{
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2024,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": [2,"always-multiline"],
    "indent": [2,4],
    "quotes": ["error", "single"],
    "no-unused-vars": "warn"
  }
}